{
  "presets": [
      "@babel/preset-env",
      "@babel/preset-react"
  ],
  "plugins": [
      [
        "@babel/plugin-proposal-class-properties",
        ["@babel/plugin-transform-runtime", {
          "corejs": true,
          "helpers": true,
          "regenerator": true,
          "useESModules": false
        }]
      ]
  ]
}